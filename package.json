{
  "name": "silkyjs",
  "main": "./dist/Silky.min.js",
  "version": "0.1.0",
  "description": "This is a simple mvvm library for javascript",
  "keywords": [
    "data-bind",
    "mvvm",
    "bind"
  ],
  "repository": "https://github.com/rail-rain/Silky.js",
  "license": "MIT",
  "scripts": {
    "watch": "watchify src/index.js -o dist/Silky.js",
    "build": "browserify src/index.js -d -p [minifyify  --no-map] > dist/Silky.min.js"
  },
  "dependencies": {
    "html2template-hs": "^0.4.1",
    "object.observe": "^0.2.4",
    "observe-deep": "^0.1.0",
    "virtual-dom": "^2.0.1"
  },
  "devDependencies": {
    "browserify": "^11.0.0",
    "minifyify": "^7.0.3",
    "watchify": "^3.3.0"
  }
}
